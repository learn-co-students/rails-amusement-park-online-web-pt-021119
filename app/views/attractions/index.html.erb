<h1>Attractions</h1><br><br>


<% Attraction.all.each do |a| %>
  <% if @user.admin %>
    <%= link_to "Show #{a.name}", attraction_path(a) %><br><br>
  <% else %>
    <%= link_to "Go on #{a.name}", attraction_path(a) %><br><br>
  <% end %>
<% end %>

<br><br><h3>
<% if @user.admin %>
  <%= link_to "New Attraction", new_attraction_path %>
<% end %>
</h3>
